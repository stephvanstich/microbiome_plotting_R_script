
#Plot heatmap:

pdf("Mothers heatmap.pdf", width = 20)
plot_heatmap(Filtertaxa100.mothers, "NMDS", "bray", "Description", "Class", sample.order = "Description", taxa.order = "Phylum")
dev.off()

